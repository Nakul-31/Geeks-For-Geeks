class Solution:
    def reverse(self, head):
        if not head or not head.next:
            return head

        curr = head
        temp = None

        # Traverse and swap prev & next for each node
        while curr:
            temp = curr.prev
            curr.prev = curr.next
            curr.next = temp
            curr = curr.prev  # move to next node (which is prev due to swap)

        # New head is last processed node's prev
        if temp:
            head = temp.prev

        return head

